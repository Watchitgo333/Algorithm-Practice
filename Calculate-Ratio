function plusMinus(arr) {
    var neg = 0;
    var pos = 0;
    var zero = 0;
    for(var i = 0; i < arr.length; i ++) {
        if(arr[i] < 0) {
            neg += 1;
        }
        else if(arr[i] > 0) {
            pos += 1;
        }
        else if (arr[i] == 0) {
            zero += 1;
        }
    }
        console.log(pos/arr.length);
        console.log(neg/arr.length);
        console.log(zero/arr.length);
}
